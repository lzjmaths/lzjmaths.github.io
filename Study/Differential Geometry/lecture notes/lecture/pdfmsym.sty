\def\@@Arrow@rule#1#2#3{%
    \lower\dimexpr #2 * #3 * \@font@scale\relax
    \vbox{%
        \hrule width #1 height \dimexpr \vecc@w pt * #3 * \@font@scale\relax depth 0pt%
        \kern\dimexpr #2 * #3 * \@font@scale * 2 - \vecc@w pt * #3 * \@font@scale\relax%
        \hrule width #1 height \dimexpr \vecc@w pt * #3 * \@font@scale\relax depth 0pt%
    }%
}

